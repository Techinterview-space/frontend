{
  "name": "interviewer",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "build-prod": "ng build --configuration=production",
    "serve-prod": "ng serve --configuration=production",
    "serve-staging": "ng serve --configuration=staging -o",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers ChromiumNoSandbox",
    "test-headless-ci-only": "ng test --no-watch --browsers ChromiumNoSandbox",
    "lint": "ng lint",
    "pretty": "pretty-quick",
    "pretty-all": "prettier --write \"src/**/*.ts\" \"src/**/*.html\" \"src/**/*.scss\" \"src/**/*.json\" \"e2e/**/*.ts\" \"tools/**/*.ts\" \"*.json\"",
    "bundle:analyze": "ng build --stats-json && webpack-bundle-analyzer dist/interviewer/stats.json",
    "serve:ssr:interviewer": "npm run build && node dist/interviewer/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.3.13",
    "@angular/common": "20.3.13",
    "@angular/compiler": "20.3.13",
    "@angular/core": "20.3.13",
    "@angular/forms": "20.3.13",
    "@angular/platform-browser": "20.3.13",
    "@angular/platform-browser-dynamic": "20.3.13",
    "@angular/platform-server": "20.3.13",
    "@angular/router": "20.3.13",
    "@angular/ssr": "20.3.13",
    "@auth0/auth0-angular": "^2.2.2",
    "@ng-select/ng-select": "^20.0.1",
    "@popperjs/core": "^2.11.8",
    "@sgratzl/chartjs-chart-boxplot": "^4.4.4",
    "@tailwindcss/postcss": "^4.1.18",
    "angular-auth-oidc-client": "^18.0.2",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "chart.js": "^4.4.7",
    "cytoscape": "3.33.1",
    "d3-selection": "3.0.0",
    "express": "^5.1.0",
    "husky": "^7.0.4",
    "karma-spec-reporter": "^0.0.36",
    "marked": "^16.0.0",
    "ngx-cookie-service": "^20.1.1",
    "ngx-google-analytics": "^14.0.1",
    "ngx-markdown": "^20.1.0",
    "oidc-client": "^1.11.5",
    "prettier": "^3.4.2",
    "puppeteer": "^24.2.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "zod": "^4.1.12",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "20.7.0",
    "@angular-eslint/eslint-plugin": "20.7.0",
    "@angular-eslint/eslint-plugin-template": "20.7.0",
    "@angular-eslint/schematics": "20.7.0",
    "@angular-eslint/template-parser": "20.7.0",
    "@angular/build": "20.3.11",
    "@angular/cli": "20.3.11",
    "@angular/compiler-cli": "20.3.13",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@typescript-eslint/utils": "^8.47.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.56.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "pretty-quick": "^4.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "optionalDependencies": {
    "@nx/nx-darwin-arm64": "17.2.7",
    "@nx/nx-darwin-x64": "17.2.7",
    "@nx/nx-linux-x64-gnu": "17.2.7"
  }
}
